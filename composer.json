{
    "name": "laravel/laravel",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "framework"
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2.124",
        "filament/spatie-laravel-media-library-plugin": "^3.2.124",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.32.0",
        "laravel/horizon": "^5.29.3",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0.3",
        "laravel/scout": "^10.11.7",
        "laravel/tinker": "^2.10",
        "mansoor/filament-versionable": "^0.0.7",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.3.3",
        "olssonm/l5-very-basic-auth": "^7.1",
        "php-http/curl-client": "^2.3.3",
        "predis/predis": "^2.2.2",
        "saade/filament-laravel-log": "^3.2.2",
        "saloonphp/saloon": "^3.10.0",
        "spatie/laravel-medialibrary": "^11.10.0",
        "spatie/laravel-sitemap": "^7.2.1",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-webhook-client": "^3.4.2",
        "swisnl/filament-backgrounds": "^1.1.4",
        "tightenco/ziggy": "^2.4.0",
        "typesense/typesense-php": "^4.9.3",
        "vormkracht10/laravel-open-graph-image": "^0.7.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2.2",
        "fakerphp/faker": "^1.24.0",
        "laravel/pint": "^1.18.1",
        "laravel/sail": "^1.38.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5.0",
        "pestphp/pest": ">=2.36.0",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.8.0",
        "spatie/laravel-ray": "^1.37.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan ziggy:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N -M"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "./vendor/bin/pint"
    }
}
