{
    "name": "laravel/laravel",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "framework"
    ],
    "require": {
        "php": "^8.2",
        "based/momentum-trail": "^0.3.0",
        "filament/filament": "^3.3.15",
        "filament/spatie-laravel-media-library-plugin": "^3.3.15",
        "inertiajs/inertia-laravel": "^2.0.2",
        "laravel/framework": "^11.45.0",
        "laravel/horizon": "^5.32.0",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.1.1",
        "laravel/scout": "^10.15.0",
        "laravel/tinker": "^2.10.1",
        "mansoor/filament-versionable": "^0.0.7",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.4.0",
        "olssonm/l5-very-basic-auth": "^7.2",
        "php-http/curl-client": "^2.3.3",
        "predis/predis": "^2.4.0",
        "saade/filament-laravel-log": "^3.2.3",
        "saloonphp/saloon": "^3.13.0",
        "spatie/laravel-medialibrary": "^11.12.9",
        "spatie/laravel-sitemap": "^7.3.6",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/laravel-webhook-client": "^3.4.3",
        "swisnl/filament-backgrounds": "^1.1.5",
        "typesense/typesense-php": "^4.9.3",
        "vormkracht10/laravel-open-graph-image": "^0.8.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "brianium/paratest": "^7.8.3",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.4.0",
        "laravel/pint": "^1.22.1",
        "laravel/sail": "^1.43.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.0",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-laravel": "^3.2",
        "spatie/laravel-ignition": "^2.9.1",
        "spatie/laravel-ray": "^1.40.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan trail:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --dir=\"app/Models/\" -N --write-mixin"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "analyse": "./vendor/bin/phpstan analyse --memory-limit=512M",
        "doc:models": "php artisan ide-helper:models --dir=\"app/Models/\" -N --write-mixin",
        "lint": "./vendor/bin/pint",
        "test": "php artisan test --parallel --no-coverage --recreate-databases",
        "test:lint": "./vendor/bin/pint --test"
    }
}
