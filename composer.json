{
    "name": "laravel/laravel",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "framework"
    ],
    "require": {
        "php": "^8.4",
        "backstage/laravel-og-image": "^1.1.0",
        "based/momentum-trail": "^0.4.0",
        "filament/filament": "^3.3.43",
        "filament/spatie-laravel-media-library-plugin": "^3.3.43",
        "inertiajs/inertia-laravel": "^2.0.10",
        "intervention/image-laravel": "^1.5.6",
        "intervention/validation": "^4.6.0",
        "laravel/framework": "^12.36.1",
        "laravel/horizon": "^5.38.0",
        "laravel/nightwatch": "^1.18.0",
        "laravel/octane": ">=2.13.1",
        "laravel/reverb": "^1.6.0",
        "laravel/sanctum": "^4.2.0",
        "laravel/scout": "^10.20.0",
        "laravel/tinker": "^2.10.1",
        "mansoor/filament-versionable": "^0.0.13",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.4.0",
        "olssonm/l5-very-basic-auth": "^7.2",
        "php-http/curl-client": "^2.3.3",
        "predis/predis": "^2.4.0",
        "saade/filament-laravel-log": "^3.2.3",
        "saloonphp/saloon": "^3.14.0",
        "spatie/laravel-medialibrary": "^11.17.2",
        "spatie/laravel-sitemap": "^7.3.7",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/laravel-webhook-client": "^3.4.4",
        "swisnl/filament-backgrounds": "^1.1.5",
        "symfony/http-client": ">=7.3.4",
        "team-reflex/discord-php": "^10.40.6",
        "typesense/typesense-php": "^4.9.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6.0",
        "brianium/paratest": "^7.8.4",
        "driftingly/rector-laravel": "^2.1.2",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.8.0",
        "laravel/pint": "^1.25.1",
        "laravel/sail": "^1.47.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "nunomaduro/phpinsights": "^2.13.3",
        "pestphp/pest": "^3.8.4",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-type-coverage": "^3.6.1",
        "rector/rector": "^2.2.7",
        "spatie/laravel-ignition": "^2.9.1",
        "spatie/laravel-ray": "^1.41.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan trail:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --dir=\"app/Models/\" -RW",
            "rector",
            "./vendor/bin/pint"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "doc:models": "php artisan ide-helper:models --dir=\"app/Models/\" -RW && rector && ./vendor/bin/pint",
        "lint": "./vendor/bin/pint",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:refactor",
            "@test:types",
            "@test:type-coverage",
            "@test:unit"
        ],
        "test:arch": "pest --filter=arch",
        "test:lint": "./vendor/bin/pint --test",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --recreate-databases --type-coverage --min=98",
        "test:types": "./vendor/bin/phpstan analyse --memory-limit=512M",
        "test:unit": "pest --parallel --recreate-databases --coverage --min=80"
    }
}
